<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.parker.user.dao.QuestionReplyDAO">

   <select id="questionReply" parameterType="com.parker.user.vo.QuestionReplyVO"
      resultType="com.parker.user.vo.QuestionReplyVO">
      select * from
      questionReply_tb
      where question_number =
      #{question_number}
   </select>

</mapper>
