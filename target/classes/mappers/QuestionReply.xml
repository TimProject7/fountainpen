  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.parker.user.dao.QuestionReplyDAO">

	<select id="questionReply" parameterType="com.paker.user.vo.QeustionReplyVO"
		resultType="com.paker.user.vo.QeustionReplyVO">
